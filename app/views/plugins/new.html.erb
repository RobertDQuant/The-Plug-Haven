<h1> New Plugin</h1>


<%= form_for :plugin, url: plugins_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :year %><br>
    <%= f.text_field :year %>
  </p>

  <p>
    <%= f.label :creator %><br>
    <%= f.text_field :creator %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.label :video_Link %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :direct_Link %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', plugins_path %>
