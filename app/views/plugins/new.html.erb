<h1> New Plugin</h1>


<%= form_for :plugin do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :year %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :creator %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :text %>
  </p>
  
  <p>
    <%= f.label :video_Link %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :direct_Link %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>